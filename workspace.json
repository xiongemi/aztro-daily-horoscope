{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react-native"
  },
  "defaultProject": "daily-horoscope",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none"
      },
      "library": {
        "style": "none",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "services": {
      "root": "libs/services",
      "sourceRoot": "libs/services/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/services/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/services"],
          "options": {
            "jestConfig": "libs/services/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "daily-horoscope": {
      "root": "apps/daily-horoscope",
      "sourceRoot": "apps/daily-horoscope/src",
      "projectType": "application",
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start daily-horoscope"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "outputs": ["apps/daily-horoscope/build"],
          "options": {
            "entryFile": "apps/daily-horoscope/src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/apps/daily-horoscope/ios/main.bundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/daily-horoscope/android/app/build/outputs/bundle",
            "apps/daily-horoscope/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/daily-horoscope/src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/apps/daily-horoscope/android/main.bundle"
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/daily-horoscope/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/daily-horoscope"],
          "options": {
            "jestConfig": "apps/daily-horoscope/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "daily-horoscope-web": {
      "root": "apps/daily-horoscope-web",
      "sourceRoot": "apps/daily-horoscope-web/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/daily-horoscope-web",
            "index": "apps/daily-horoscope-web/src/index.html",
            "main": "apps/daily-horoscope-web/src/main.tsx",
            "polyfills": "apps/daily-horoscope-web/src/polyfills.ts",
            "tsConfig": "apps/daily-horoscope-web/tsconfig.app.json",
            "assets": [
              "apps/daily-horoscope-web/src/favicon.ico",
              "apps/daily-horoscope-web/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/daily-horoscope-web/src/environments/environment.ts",
                  "with": "apps/daily-horoscope-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "daily-horoscope-web:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "daily-horoscope-web:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/daily-horoscope-web/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/daily-horoscope-web"],
          "options": {
            "jestConfig": "apps/daily-horoscope-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "daily-horoscope-web-e2e": {
      "root": "apps/daily-horoscope-web-e2e",
      "sourceRoot": "apps/daily-horoscope-web-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/daily-horoscope-web-e2e/cypress.json",
            "tsConfig": "apps/daily-horoscope-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "daily-horoscope-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "daily-horoscope-web:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/daily-horoscope-web-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
